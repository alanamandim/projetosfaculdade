package poo;
import edu.princeton.cs.algs4.StdDraw;

public class Principal {

    public static void main(String[] args) {
        
        // Lendo argumentos de linha de comando
        if (args.length > 0 ) {
            StdDraw.text(.3, .3, args[0]);
        }

       StdDraw.rectangle(0.5, 0.5, 0.05, 0.05);
    }
}
